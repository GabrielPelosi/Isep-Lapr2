               <padding>
                  <Insets left="20.0" />
               </padding>
            </HBox>
            <Button fx:id="butaoSair" maxWidth="130.0" mnemonicParsing="false" onAction="#clicarSair" prefHeight="25.0" prefWidth="130.0" text="EXIT" />
