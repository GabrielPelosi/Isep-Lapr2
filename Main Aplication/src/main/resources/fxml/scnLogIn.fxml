               </VBox.margin></Text>
            <TextField fx:id="fieldEmail" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="130.0" promptText="email" />
            <PasswordField fx:id="fieldPass" maxWidth="-Infinity" prefWidth="130.0" promptText="password" />
            <Button maxWidth="130.0" mnemonicParsing="false" onAction="#clicarLogIn" prefHeight="25.0" prefWidth="130.0" text="Log In" />
            <HBox alignment="CENTER" prefHeight="48.0" prefWidth="600.0" spacing="5.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#registerAsCustomer" text="Register" />
                  <Button mnemonicParsing="false" onAction="#butRegisterSP" prefHeight="43.0" prefWidth="108.0" text="Register as Service Provider" textAlignment="CENTER" wrapText="true">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
